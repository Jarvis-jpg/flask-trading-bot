{
  "enabled": true,
  "frequency_hours": 24,
  "retention_days": 30,
  "backup_location": "backups/",
  "include_files": [
    "*.py",
    "*.json",
    "*.log",
    "*.pine",
    ".env",
    "requirements.txt"
  ],
  "exclude_patterns": [
    "__pycache__/",
    "*.pyc",
    "temp/",
    "logs/debug.log"
  ]
}